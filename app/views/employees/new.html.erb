<h1>New Recipe<h1>

<%= form_for @employee do |f| %>

    <%= f.label :first_name, "Employee First Name" %>
    <%= f.text_field :first_name %>
    <br>
    <%= f.label :last_name, "Employee Last Name" %>
    <%= f.text_field :last_name %>
    <br>
    <%= f.label :office %>
    <%= f.text_field :office %>
    <br>
    <%= f.label :alias %>
    <%= f.text_field :alias %>
    <br>
    <%= f.label :title %>
    <%= f.text_field :title %>
    <br>
    <%= f.label :image %>
    <%= f.file_field :img_url %>
    <br>
    <%= f.collection_select :dog_id, Dog.all, :id, :name %>
    <br>
    <%= f.submit "Add Song"%>

  
<% end %>